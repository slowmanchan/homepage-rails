<div class="container light-blue-wrapper">
  <div class="wrapper">


  <% @posts.each do |post| %>

    <div class="post-wrapper">
      <h1><%= link_to post.title, post %></h1>
        <p><%= post.created_at.strftime("%B, %d, %Y") %></p>
        <p><%= post.body %></p>
      <hr>
    </div>

  <% end %>
 </div>
</div>
